<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
    <meta charset="utf-8">
    <title>OnBus</title>
    <meta charset="utf-8">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="/public/css/ticket.css">
</head>


<body  style=" background:  linear-gradient(135deg, #fe2929, #ffb3c5) " >

    <div  >
        <div class="header-blue">
            <nav class="navbar navbar-light navbar-expand-md navigation-clean-search">
                <div class="container-fluid"><h1 class="navbar-brand text-white " >BUS TICKET </h1><button
                        data-toggle="collapse" class="navbar-toggler" data-target="#navcol-1"><span
                            class="sr-only">Toggle navigation</span><span class="navbar-toggler-icon"></span></button>
                    <div class="collapse navbar-collapse" id="navcol-1">
                        <ul class="nav navbar-nav">
                            <li class="nav-item" role="presentation"><a class="nav-link text-white" href="/">Home</a></li>
                            <li class="nav-item" role="presentation"><a class="nav-link text-white" href="/home">Get Tickets</a></li>
                        </ul>  
                    </div>
                </div>
            </nav>
            <h3 class="font-weight-bold text-white " style="margin-left: 40%; margin-right: 40%;">Tickets</h3>
           
        </div>
        <% tickets.forEach(function(ticket){ %>
        <div class="py-5">
            <div class="container">    
              <div class="row hidden-md-up ">
                <div class="col-md-7 col-lg-7 mx-auto  ">
                  <div class="card p-3">
                    <div class="card-block ">
                      <h4  class="card-title"> ID: <%=ticket.ticketId%></h4>
                      <h6 class="card-subtitle  mb-3">From:  <%=ticket.from%></h6>
                      <h6 class="card-subtitle  mb-3">To:  <%=ticket.to%></h6>
                      <h6 class="card-subtitle  mb-3">DATE:  <%=ticket.date%></h6> 
                      <h5>Total Ticket: <%=ticket.ticket_count%></h5>
                      <h5>Amount: <%=ticket.amount%></h5>
                    </div>
                  </div>
                </div>
            </div>
            </div>
            <% }) %>    
    </div>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script type="text/javascript"
        src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
</body>

</html>

















